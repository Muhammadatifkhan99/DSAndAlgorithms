.PHONY: clean All

All:
	@echo "----------Building project:[ ReversingAString - Debug ]----------"
	@cd "ReversingAString" && "$(MAKE)" -f  "ReversingAString.mk"
clean:
	@echo "----------Cleaning project:[ ReversingAString - Debug ]----------"
	@cd "ReversingAString" && "$(MAKE)" -f  "ReversingAString.mk" clean
