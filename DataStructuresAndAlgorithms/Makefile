.PHONY: clean All

All:
	@echo "----------Building project:[ LengthOfString - Debug ]----------"
	@cd "LengthOfString" && "$(MAKE)" -f  "LengthOfString.mk"
clean:
	@echo "----------Cleaning project:[ LengthOfString - Debug ]----------"
	@cd "LengthOfString" && "$(MAKE)" -f  "LengthOfString.mk" clean
